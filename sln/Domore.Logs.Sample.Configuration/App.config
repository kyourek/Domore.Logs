<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="logging.handler[log]" value="log file"/>
    <add key="log file . directory" value="logs"/>
    <add key="log file .  severity" value="warn"/>
    <add key="log file .    format" value="{Time:HH:mm:ss} {Name} [{Severity}] {Data}"/>
  </appSettings>
</configuration>